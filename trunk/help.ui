&lt;li style=&quot; font-size:9pt;&quot; style=&quot; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;&quot;&gt;&lt;span style=&quot; font-weight:600;&quot;&gt;gzip -dc cd_iso.gz | cdrecord dev=/dev/cdrom -&lt;/span&gt;: quemar una imagen iso comprimida.&lt;/li&gt;
